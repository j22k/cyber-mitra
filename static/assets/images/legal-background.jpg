<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 600">
    <style>
        .dark { fill: #2d3748; }
        .accent { fill: #b69566; }
        .light-accent { fill: #e4d4ba; }
        .white { fill: #ffffff; opacity: 0.05; }
    </style>
    
    <!-- Background -->
    <rect width="1200" height="600" fill="#1a202c"/>
    
    <!-- Courthouse Columns -->
    <g opacity="0.2">
        <!-- Left Columns -->
        <rect class="white" x="50" y="100" width="40" height="400"/>
        <rect class="white" x="50" y="80" width="40" height="20" rx="2"/>
        <rect class="white" x="50" y="500" width="40" height="20" rx="2"/>
        
        <rect class="white" x="150" y="100" width="40" height="400"/>
        <rect class="white" x="150" y="80" width="40" height="20" rx="2"/>
        <rect class="white" x="150" y="500" width="40" height="20" rx="2"/>
        
        <rect class="white" x="250" y="100" width="40" height="400"/>
        <rect class="white" x="250" y="80" width="40" height="20" rx="2"/>
        <rect class="white" x="250" y="500" width="40" height="20" rx="2"/>
        
        <!-- Right Columns -->
        <rect class="white" x="910" y="100" width="40" height="400"/>
        <rect class="white" x="910" y="80" width="40" height="20" rx="2"/>
        <rect class="white" x="910" y="500" width="40" height="20" rx="2"/>
        
        <rect class="white" x="1010" y="100" width="40" height="400"/>
        <rect class="white" x="1010" y="80" width="40" height="20" rx="2"/>
        <rect class="white" x="1010" y="500" width="40" height="20" rx="2"/>
        
        <rect class="white" x="1110" y="100" width="40" height="400"/>
        <rect class="white" x="1110" y="80" width="40" height="20" rx="2"/>
        <rect class="white" x="1110" y="500" width="40" height="20" rx="2"/>
        
        <!-- Top and Bottom Borders -->
        <rect class="white" x="30" y="60" width="1140" height="20" rx="2"/>
        <rect class="white" x="30" y="520" width="1140" height="20" rx="2"/>
    </g>
    
    <!-- Legal Patterns -->
    <g opacity="0.1">
        <!-- Scales of Justice Outlines -->
        <g transform="translate(350, 150) scale(1.5)">
            <path class="accent" d="M50,20 C70,10 80,30 70,40 C60,50 40,50 30,40 C20,30 30,10 50,20 Z M150,20 C170,10 180,30 170,40 C160,50 140,50 130,40 C120,30 130,10 150,20 Z M100,10 L100,100 M50,20 L50,80 M150,20 L150,80 M30,40 L70,40 M130,40 L170,40" fill="none" stroke-width="2"/>
            <rect class="accent" x="90" y="100" width="20" height="10" rx="2"/>
        </g>
        
        <g transform="translate(800, 400) scale(1.5)">
            <path class="accent" d="M50,20 C70,10 80,30 70,40 C60,50 40,50 30,40 C20,30 30,10 50,20 Z M150,20 C170,10 180,30 170,40 C160,50 140,50 130,40 C120,30 130,10 150,20 Z M100,10 L100,100 M50,20 L50,80 M150,20 L150,80 M30,40 L70,40 M130,40 L170,40" fill="none" stroke-width="2"/>
            <rect class="accent" x="90" y="100" width="20" height="10" rx="2"/>
        </g>
        
        <!-- Gavel Outlines -->
        <g transform="translate(600, 250)">
            <rect class="light-accent" x="0" y="0" width="60" height="20" rx="5"/>
            <rect class="light-accent" x="70" y="5" width="100" height="10" rx="2"/>
        </g>
        
        <g transform="translate(200, 450)">
            <rect class="light-accent" x="0" y="0" width="60" height="20" rx="5"/>
            <rect class="light-accent" x="70" y="5" width="100" height="10" rx="2"/>
        </g>
        
        <!-- Law Book Stacks -->
        <g transform="translate(500, 100)">
            <rect class="white" x="0" y="0" width="80" height="20" rx="2"/>
            <rect class="white" x="5" y="20" width="80" height="20" rx="2"/>
            <rect class="white" x="10" y="40" width="80" height="20" rx="2"/>
            <rect class="white" x="15" y="60" width="80" height="20" rx="2"/>
        </g>
        
        <g transform="translate(700, 350)">
            <rect class="white" x="0" y="0" width="80" height="20" rx="2"/>
            <rect class="white" x="5" y="20" width="80" height="20" rx="2"/>
            <rect class="white" x="10" y="40" width="80" height="20" rx="2"/>
            <rect class="white" x="15" y="60" width="80" height="20" rx="2"/>
        </g>
        
        <g transform="translate(200, 300)">
            <rect class="white" x="0" y="0" width="80" height="20" rx="2"/>
            <rect class="white" x="5" y="20" width="80" height="20" rx="2"/>
            <rect class="white" x="10" y="40" width="80" height="20" rx="2"/>
            <rect class="white" x="15" y="60" width="80" height="20" rx="2"/>
        </g>
    </g>
    
    <!-- Floating Binary Code -->
    <g opacity="0.2">
        <text class="light-accent" x="100" y="150" font-family="monospace" font-size="12">01010011 01100101 01110010 01110110 01101001</text>
        <text class="light-accent" x="400" y="200" font-family="monospace" font-size="12">01001100 01100101 01100111 01100001 01101100</text>
        <text class="light-accent" x="700" y="250" font-family="monospace" font-size="12">01001010 01110101 01110011 01110100 01101001</text>
        <text class="light-accent" x="300" y="300" font-family="monospace" font-size="12">01000011 01111001 01100010 01100101 01110010</text>
        <text class="light-accent" x="600" y="350" font-family="monospace" font-size="12">01001101 01101001 01110100 01110010 01100001</text>
        <text class="light-accent" x="200" y="400" font-family="monospace" font-size="12">01000001 01001001 00100000 01001100 01100001</text>
        <text class="light-accent" x="500" y="450" font-family="monospace" font-size="12">01000010 01101111 01100101 01101000 01101101</text>
        <text class="light-accent" x="800" y="500" font-family="monospace" font-size="12">01010100 01100101 01100011 01101000 01101110</text>
    </g>
    
    <!-- Digital Circuit Lines -->
    <g opacity="0.15">
        <path class="accent" d="M0,100 L1200,100 M0,200 L1200,200 M0,300 L1200,300 M0,400 L1200,400 M0,500 L1200,500" stroke-width="1" stroke-dasharray="10,10" fill="none"/>
        <path class="accent" d="M100,0 L100,600 M300,0 L300,600 M500,0 L500,600 M700,0 L700,600 M900,0 L900,600 M1100,0 L1100,600" stroke-width="1" stroke-dasharray="10,10" fill="none"/>
    </g>
    
    <!-- Circuit Nodes -->
    <g>
        <circle class="accent" cx="100" cy="100" r="4" opacity="0.3"/>
        <circle class="accent" cx="300" cy="100" r="4" opacity="0.3"/>
        <circle class="accent" cx="500" cy="100" r="4" opacity="0.3"/>
        <circle class="accent" cx="700" cy="100" r="4" opacity="0.3"/>
        <circle class="accent" cx="900" cy="100" r="4" opacity="0.3"/>
        <circle class="accent" cx="1100" cy="100" r="4" opacity="0.3"/>
        
        <circle class="accent" cx="100" cy="200" r="4" opacity="0.3"/>
        <circle class="accent" cx="300" cy="200" r="4" opacity="0.3"/>
        <circle class="accent" cx="500" cy="200" r="4" opacity="0.3"/>
        <circle class="accent" cx="700" cy="200" r="4" opacity="0.3"/>
        <circle class="accent" cx="900" cy="200" r="4" opacity="0.3"/>
        <circle class="accent" cx="1100" cy="200" r="4" opacity="0.3"/>
        
        <circle class="accent" cx="100" cy="300" r="4" opacity="0.3"/>
        <circle class="accent" cx="300" cy="300" r="4" opacity="0.3"/>
        <circle class="accent" cx="500" cy="300" r="4" opacity="0.3"/>
        <circle class="accent" cx="700" cy="300" r="4" opacity="0.3"/>
        <circle class="accent" cx="900" cy="300" r="4" opacity="0.3"/>
        <circle class="accent" cx="1100" cy="300" r="4" opacity="0.3"/>
        
        <circle class="accent" cx="100" cy="400" r="4" opacity="0.3"/>
        <circle class="accent" cx="300" cy="400" r="4" opacity="0.3"/>
        <circle class="accent" cx="500" cy="400" r="4" opacity="0.3"/>
        <circle class="accent" cx="700" cy="400" r="4" opacity="0.3"/>
        <circle class="accent" cx="900" cy="400" r="4" opacity="0.3"/>
        <circle class="accent" cx="1100" cy="400" r="4" opacity="0.3"/>
        
        <circle class="accent" cx="100" cy="500" r="4" opacity="0.3"/>
        <circle class="accent" cx="300" cy="500" r="4" opacity="0.3"/>
        <circle class="accent" cx="500" cy="500" r="4" opacity="0.3"/>
        <circle class="accent" cx="700" cy="500" r="4" opacity="0.3"/>
        <circle class="accent" cx="900" cy="500" r="4" opacity="0.3"/>
        <circle class="accent" cx="1100" cy="500" r="4" opacity="0.3"/>
    </g>
    
    <!-- Paragraph text imitation -->
    <g opacity="0.1">
        <rect class="white" x="400" y="120" width="400" height="2"/>
        <rect class="white" x="400" y="130" width="380" height="2"/>
        <rect class="white" x="400" y="140" width="390" height="2"/>
        <rect class="white" x="400" y="150" width="370" height="2"/>
        <rect class="white" x="400" y="160" width="400" height="2"/>
        
