        <rect class="white" x="400" y="350" width="370" height="2"/>
        <rect class="white" x="400" y="360" width="400" height="2"/>
    </g>
    
    <!-- AI and Tech Elements -->
    <g opacity="0.15">
        <!-- CPU Grid -->
        <rect class="white" x="300" y="220" width="100" height="100"/>
        <rect class="white" x="310" y="230" width="10" height="10"/>
        <rect class="white" x="330" y="230" width="10" height="10"/>
        <rect class="white" x="350" y="230" width="10" height="10"/>
        <rect class="white" x="370" y="230" width="10" height="10"/>
        
        <rect class="white" x="310" y="250" width="10" height="10"/>
        <rect class="white" x="330" y="250" width="10" height="10"/>
        <rect class="white" x="350" y="250" width="10" height="10"/>
        <rect class="white" x="370" y="250" width="10" height="10"/>
        
        <rect class="white" x="310" y="270" width="10" height="10"/>
        <rect class="white" x="330" y="270" width="10" height="10"/>
        <rect class="white" x="350" y="270" width="10" height="10"/>
        <rect class="white" x="370" y="270" width="10" height="10"/>
        
        <rect class="white" x="310" y="290" width="10" height="10"/>
        <rect class="white" x="330" y="290" width="10" height="10"/>
        <rect class="white" x="350" y="290" width="10" height="10"/>
        <rect class="white" x="370" y="290" width="10" height="10"/>
        
        <!-- Neural Network -->
        <g transform="translate(800, 150)">
            <!-- Nodes -->
            <circle class="white" cx="20" cy="20" r="5"/>
            <circle class="white" cx="20" cy="50" r="5"/>
            <circle class="white" cx="20" cy="80" r="5"/>
            
            <circle class="white" cx="60" cy="20" r="5"/>
            <circle class="white" cx="60" cy="50" r="5"/>
            <circle class="white" cx="60" cy="80" r="5"/>
            
            <circle class="white" cx="100" cy="35" r="5"/>
            <circle class="white" cx="100" cy="65" r="5"/>
            
            <circle class="white" cx="140" cy="50" r="5"/>
            
            <!-- Connections -->
            <line x1="20" y1="20" x2="60" y2="20" stroke="#b69566" stroke-width="1"/>
            <line x1="20" y1="20" x2="60" y2="50" stroke="#b69566" stroke-width="1"/>
            <line x1="20" y1="20" x2="60" y2="80" stroke="#b69566" stroke-width="1"/>
            
            <line x1="20" y1="50" x2="60" y2="20" stroke="#b69566" stroke-width="1"/>
            <line x1="20" y1="50" x2="60" y2="50" stroke="#b69566" stroke-width="1"/>
            <line x1="20" y1="50" x2="60" y2="80" stroke="#b69566" stroke-width="1"/>
            
            <line x1="20" y1="80" x2="60" y2="20" stroke="#b69566" stroke-width="1"/>
            <line x1="20" y1="80" x2="60" y2="50" stroke="#b69566" stroke-width="1"/>
            <line x1="20" y1="80" x2="60" y2="80" stroke="#b69566" stroke-width="1"/>
            
