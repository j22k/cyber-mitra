            <line x1="100" y1="65" x2="140" y2="50" stroke="#b69566" stroke-width="1"/>
        </g>
    </g>
    
    <!-- Overlay Texture -->
    <rect width="1200" height="600" fill="url(#noise)" opacity="0.02"/>
    
    <!-- Noise Pattern -->
    <defs>
        <pattern id="noise" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
            <rect width="100" height="100" fill="#ffffff"/>
            <rect width="50" height="50" fill="#000000" opacity="0.1"/>
            <rect x="50" y="50" width="50" height="50" fill="#000000" opacity="0.1"/>
        </pattern>
    </defs>
</svg>